<Window
    x:Class="FitnessClubMWWM.Ui.Desktop.Pages.Wind.SalariesEmployeesWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:FitnessClubMWWM.Ui.Desktop.Pages.Wind"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="SalariesEmployeesWindow"
    Width="400"
    Height="300"
    AllowsTransparency="True"
    DataContext="{Binding StaffPaymentPageViewModel, Source={StaticResource Locator}}"
    Style="{StaticResource SalariesOfEmployeesWindowStyle}"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">
    <WindowChrome.WindowChrome>
        <WindowChrome
            CaptionHeight="20"
            CornerRadius="0"
            GlassFrameThickness="0"
            ResizeBorderThickness="0" />
    </WindowChrome.WindowChrome>
</Window>
